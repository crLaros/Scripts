	Name = "Reset All Stats To 10",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = 10,[4] = 10}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
        wait(1)
        local args = {[1] = "Health",[2] = "One",[3] = 10,[4] = 10}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
        wait(1)
        local args = {[1] = "Loot",[2] = "One",[3] = 10,[4] = 10}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
