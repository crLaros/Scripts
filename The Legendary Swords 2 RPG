--Tab 4 Currencies

local Gold = nil
local SkyShards = nil

tab4:Textbox("Custom Gold",false, function(value)
    Gold = value
    Gold = Gold * 2
end)

tab4:Button("Get Gold", function()
    local args = {[1] = "Buy",[2] = "Gold",[3] = 0,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
    wait(2)
    local args = {[1] = "Sell",[2] = "Gold",[3] = Gold,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
end)

tab4:Textbox("Custom Sky Shards",false, function(value)
    SkyShards = value
    SkyShards = SkyShards * 2
end)

tab4:Button("Get Sky Shards", function()
    local args = {[1] = "Buy",[2] = "Shards",[3] = 0,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
    wait(2)
    local args = {[1] = "Sell",[2] = "Shards",[3] = SkyShards,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
end)

--Tab 5 Teleport

local TeleSpot = nil
local TeleEnemy = nil
local TeleBoss = nil

tab5:Dropdown("Choose Spot",{"Shop 1", "Shop 2","Shop 3","Guild","Tower","Mine","Forbidden Temple","Devils Fortress","Ice Realm"}, function(value)
    TeleSpot = value
end)

tab5:Button("Teleport To Spot", function()
    if TeleSpot == "Shop 1" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-732, 14, -616))
    elseif TeleSpot == "Shop 2" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-862, 41, 442))
    elseif TeleSpot == "Shop 3" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(791, 15, 590))
    elseif TeleSpot == "Guild" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-325, 17, 696))
    elseif TeleSpot == "Tower" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(555, 68, -767))
    elseif TeleSpot == "Forbidden Temple" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-852, 39, 766))
    elseif TeleSpot == "Ice Realm" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-76, 20, 910))
    elseif TeleSpot == "Devils Fortress" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(750, 26, 892))
    elseif TeleSpot == "Mine" then
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-543, 10, -666))
    end
end)

