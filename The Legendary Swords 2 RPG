Tab2:AddToggle({
	Name = "Auto Upgrade Mining Level",
	Default = false,
	Callback = function(bool)
		getgenv().autoMininglvl = bool;
        if bool then
            autoMininglvl()
        end
	end
})

function autoMininglvl()
	while getgenv().autoMininglvl == true do
		local args = {[1] = 0}
		game:GetService("Players").LocalPlayer.RemoteFunctions.MiningLvlSystem:FireServer(unpack(args))
		wait(0.5)
	end
end

print("Made by crLaros")

--Tab 3 Rebirth

getgenv().autoRebirth = false

function autoRebirth()
    while getgenv().autoRebirth == true do
        game.Players.LocalPlayer.RemoteFunctions.RebirthSystem:FireServer()
        wait(0.5)
    end
end

Tab3:AddButton({
	Name = "Rebirth",
	Callback = function()
        game.Players.LocalPlayer.RemoteFunctions.RebirthSystem:FireServer()
  	end
})

Tab3:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(bool)
		getgenv().autoRebirth = bool;
        if bool then
            autoRebirth()
        end
	end
})

