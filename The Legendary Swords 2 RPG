	Name = "Get Gold",
	Callback = function()
        local args = {[1] = "Buy",[2] = "Gold",[3] = 0,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
        wait(2)
        local args = {[1] = "Sell",[2] = "Gold",[3] = MONEY,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
  	end
})


Tab6:AddTextbox({
	Name = "How Much Skyshards",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		SKYSHARDS = (Value)
        SKYSHARDS = SKYSHARDS * 2
	end
})

