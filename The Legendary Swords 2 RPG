tab2:Toggle("Auto Rebirth",false, function(bool)
    getgenv().FarmRebirth = bool
    if bool then
        FarmRebirth()
    end
end)

function FarmRebirth()
    while getgenv().FarmRebirth == true do
        game.Players.LocalPlayer.RemoteFunctions.RebirthSystem:FireServer()
        wait(0.5)
    end
end

