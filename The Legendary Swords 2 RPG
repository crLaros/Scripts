--Tab 6 Misc

getgenv().farmores = false
getgenv().noclip = false
getgenv().autoMininglvl = false
local Speed = 16
local Jump = 50


tab6:Slider("Walk Speed",16,300,23, function(value)
    Speed = value
	setscriptable(game.Players.LocalPlayer.Character.Humanoid,"WalkSpeed",false)
	sethiddenproperty(game.Players.LocalPlayer.Character.Humanoid,"WalkSpeed",Speed)
end)

tab6:Slider("Jump Power",50,1000,50, function(value)
    Jump = value
    setscriptable(game.Players.LocalPlayer.Character.Humanoid,"JumpPower",false)
	sethiddenproperty(game.Players.LocalPlayer.Character.Humanoid,"JumpPower",Jump)
end)

tab6:Label("You Need Mining LV.6 For All")
tab6:Toggle("Farm Ores",false, function(bool)
    getgenv().farmores = bool
    if bool then
        FarmOres()
    end
end)

tab6:Toggle("Auto Upgrade Mining Level",false, function(bool)
    getgenv().autoMininglvl = bool
    if bool then
        autoMininglvl()
    end
end)

tab6:Toggle("No Clip",false, function(bool)
    getgenv().noclip = bool
    if bool then
        noclip()
    end
end)

function autoMininglvl()
	while getgenv().autoMininglvl == true do
		local args = {[1] = 0}
		game:GetService("Players").LocalPlayer.RemoteFunctions.MiningLvlSystem:FireServer(unpack(args))
		wait(0.5)
	end
end

function FarmOres()
	while wait() do
		if not getgenv().farmores then break end
		for i, v in pairs(game:GetService("Workspace")["Parts/Scenary/Terrain"]:GetDescendants()) do
			if v.Name == "CanMine" and v.Parent then
				firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
				wait()
				firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
				wait()
			end
		end
	end
end

function noclip()
	if getgenv().noclip == true then
		local loop game:GetService('RunService').Stepped:connect(function()
			if getgenv().noclip then
				for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
					if v:IsA("BasePart") then
						if v.CanCollide ~= not getgenv().noclip then
						v.CanCollide = not getgenv().noclip
						end
					end
				end
			end
		end)
	end
end

--Tab 7 Settings

