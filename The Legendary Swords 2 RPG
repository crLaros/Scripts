--Tab 3 Rebirth

getgenv().autoRebirth = false

function autoRebirth()
    spawn(function ()
        while getgenv().autoRebirth == true do
            game.Players.LocalPlayer.RemoteFunctions.RebirthSystem:FireServer()
            wait(0.5)
        end
    end)
end

Tab3:AddButton({
	Name = "Rebirth",
	Callback = function()
        game.Players.LocalPlayer.RemoteFunctions.RebirthSystem:FireServer()
  	end
})

Tab3:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(bool)
		getgenv().autoRebirth = bool;
        if bool then
            autoRebirth()
        end
	end
})

--Tab 4 Weapons

getgenv().Swords = nil


Tab4:AddDropdown({
	Name = "Choose Weapon",
	Default = "",
	Options = {"Dark Axe", "Shai Sword","Mystic Sword","Exotic Pacifer","Redcliff Pacifier"},
	Callback = function(Value)
		getgenv().Swords1 = Value
	end
})

Tab4:AddButton({
	Name = "Get Weapon",
	Callback = function()
        local args = {[1] = "Buy",[2] = "Gold",[3] = 0,[4] = getgenv().Swords1}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
  	end
})

--Tab 5 Armor

getgenv().armor = nil

Tab5:AddDropdown({
	Name = "Choose Armor",
	Default = "",
	Options = {"Dark Samurai", "Phoenix Guard","Dark Overseer","Iron","Fire Forged","Elite","Sapphire","Korblox Ice","Doom Bringer"},
	Callback = function(Value)
		getgenv().armor = Value
	end
})

Tab5:AddButton({
	Name = "Equip Armor",
	Callback = function()
        local args = {[1] = getgenv().armor}; game:GetService("Players").LocalPlayer.RemoteFunctions.ArmorSystem:FireServer(unpack(args))
  	end
})

--Tab 6 Currencies
getgenv().MONEY = nil
getgenv().SKYSHARDS = nil

Tab6:AddTextbox({
	Name = "How Much Money",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().MONEY = (Value)
        getgenv().MONEY = getgenv().MONEY * 2
	end
})

Tab6:AddButton({
	Name = "Get Gold",
	Callback = function()
        local args = {[1] = "Buy",[2] = "Gold",[3] = 0,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
        wait(2)
        local args = {[1] = "Sell",[2] = "Gold",[3] = getgenv().MONEY,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
  	end
})


Tab6:AddTextbox({
	Name = "How Much Skyshards",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().SKYSHARDS = (Value)
        getgenv().SKYSHARDS = getgenv().SKYSHARDS * 2
	end
})

Tab6:AddButton({
	Name = "Get Skyshards",
	Callback = function()
        local args = {[1] = "Buy",[2] = "Shards",[3] = 0,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
        wait(2)
        local args = {[1] = "Sell",[2] = "Shards",[3] = getgenv().SKYSHARDS,[4] = "Rusty Axe"}; game:GetService("Players").LocalPlayer.RemoteFunctions.SwordSystem:FireServer(unpack(args))
    end
})

--Tab 7 Teleport
Tab7:AddButton({
	Name = "Teleport To Shop 1",
	Callback = function()
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-736, 14, -614))
  	end
})

Tab7:AddButton({
	Name = "Teleport To Shop 2",
	Callback = function()
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-856, 42, 439))
  	end
})

Tab7:AddButton({
	Name = "Teleport To Shop 3",
	Callback = function()
        game.Players.LocalPlayer.Character:MoveTo(Vector3.new(785, 15, 586))
  	end
})

--Tab 8 Settings

Tab8:AddButton({
	Name = "Destroy Gui!",
	Callback = function()
        game:GetService("CoreGui").Orion:Destroy()
  	end
})
