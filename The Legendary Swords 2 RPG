local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt")()
local win = lib:Window("The Legendary Swords 2 RPG",Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)
lib:Notification("Made By crLaros", "Last Update 31.10.22", "Ok")
local tab1 = win:Tab("Skillpoints")
local tab2 = win:Tab("Stats")
local tab3 = win:Tab("Weapons and Armor")
