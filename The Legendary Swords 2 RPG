local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "The Legendary Swords 2 RPG", HidePremium = false, SaveConfig = true, IntroEnabled = false})

local Tab1 = Window:MakeTab({
	Name = "Skillpoints",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Stats",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Rebirth",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab4 = Window:MakeTab({
	Name = "Weapons",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab5 = Window:MakeTab({
	Name = "Armor",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab6 = Window:MakeTab({
	Name = "Currencies",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab7 = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab8 = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



--Tab 1 Skillpoints

getgenv().SP1 = nil
getgenv().SP2 = nil

Tab1:AddDropdown({
	Name = "Choose SP",
	Default = "1",
	Options = {"10", "100","1000","10000","999999"},
	Callback = function(Value)
		getgenv().SP1 = Value
	end
})

Tab1:AddButton({
	Name = "Set SP",
	Callback = function()
        getgenv().StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
        wait(0.2)
        local args = {[1] = "Strength",[2] = "One",[3] = getgenv().StrengthFirst,[4] = getgenv().SP1}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

Tab1:AddLabel("Do not exaggerate!")

Tab1:AddTextbox({
	Name = "Custom SP",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
        getgenv().StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
        wait(0.1)
		getgenv().SP2 = Value
	end
})

Tab1:AddButton({
	Name = "Set Custom SP",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = getgenv().StrengthFirst,[4] = getgenv().SP2}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

--Tab 2 Stats
getgenv().StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
getgenv().HealthFirst = game.Players.LocalPlayer.PlayerData.Health.Value
getgenv().GoldIncreaseFirst = game.Players.LocalPlayer.PlayerData.GoldIncrease.Value
getgenv().UpgradePointsFirst = game.Players.LocalPlayer.PlayerData.UpgradePoints.Value

Tab2:AddButton({
	Name = "You get Inf Strength",
	Callback = function()
        getgenv().StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
        wait(0.1)
        getgenv().UpgradePointsFirst = game.Players.LocalPlayer.PlayerData.UpgradePoints.Value
        wait(0.1)
        local args = {[1] = "Strength",[2] = "One",[3] = 10000000000000000,[4] = getgenv().UpgradePointsFirst}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

Tab2:AddButton({
	Name = "You get Inf Health",
	Callback = function()
        getgenv().HealthFirst = game.Players.LocalPlayer.PlayerData.Health.Value
        wait(0.1)
        getgenv().UpgradePointsFirst = game.Players.LocalPlayer.PlayerData.UpgradePoints.Value
        wait(0.1)
        local args = {[1] = "Health",[2] = "One",[3] = 10000000000000000,[4] = getgenv().UpgradePointsFirst}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

Tab2:AddButton({
	Name = "You get Inf Money gain",
	Callback = function()
        getgenv().GoldIncreaseFirst = game.Players.LocalPlayer.PlayerData.GoldIncrease.Value
        wait(0.1)
        getgenv().UpgradePointsFirst = game.Players.LocalPlayer.PlayerData.UpgradePoints.Value
        wait(0.1)
        local args = {[1] = "Loot",[2] = "One",[3] = 10000000000000000,[4] = getgenv().UpgradePointsFirst}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})



