Tab7:AddLabel("Spots")

Tab7:AddDropdown({
	Name = "Choose Point",
	Default = "",
	Options = {"Shop 1", "Shop 2","Shop 3","Guild","Tower","Mine","Forbidden Temple","Devils Fortress","Ice Realm"},
	Callback = function(Value)
		TeleSpot = Value
	end
})

Tab7:AddButton({
	Name = "Teleport to Spot",
	Callback = function()
      	if TeleSpot == "Shop 1" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-732, 14, -616))
		elseif TeleSpot == "Shop 2" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-862, 41, 442))
		elseif TeleSpot == "Shop 3" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(791, 15, 590))
		elseif TeleSpot == "Guild" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-325, 17, 696))
		elseif TeleSpot == "Tower" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(555, 68, -767))
		elseif TeleSpot == "Forbidden Temple" then
			game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-852, 39, 766))
