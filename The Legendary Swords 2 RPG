local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "The Legendary Swords 2 RPG", HidePremium = false, SaveConfig = true, IntroEnabled = false})

getgenv().SP = 0
getgenv().MONEY = 0
getgenv().SKYSHARDS = 0
getgenv().autoRebirth = false

local Tab1 = Window:MakeTab({
	Name = "Skillpoints",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Stats",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Rebirth",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab4 = Window:MakeTab({
	Name = "Weapons",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab5 = Window:MakeTab({
	Name = "Armor",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab6 = Window:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Tab 1

Tab1:AddButton({
	Name = "You get 10 SP!",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = 0,[4] = 10}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end    
})

Tab1:AddButton({
	Name = "You get 100 SP!",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = 0,[4] = 100}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end    
})

