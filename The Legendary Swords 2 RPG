--First Value
StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
UpgradePointsFirst = game.Players.LocalPlayer.PlayerData.UpgradePoints.Value

--Tab 1 Skills Points

local SP1 = nil
local SP2 = nil

tab1:Dropdown("Choose SP",{"10","100","1000","10000","99999"}, function(value)
    SP1 = value
end)

tab1:Button("Set SP", function()
    StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
    wait(0.2)
    local args = {[1] = "Strength",[2] = "One",[3] = StrengthFirst,[4] = SP1}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
end)

tab1:Label("Do not exaggerate!")

tab1:Textbox("Custom SP",false, function(value)
    SP2 = value
end)

tab1:Button("Set Custom SP", function()
    StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
    wait(0.2)
    local args = {[1] = "Strength",[2] = "One",[3] = StrengthFirst,[4] = SP2}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
end)

