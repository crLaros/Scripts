--Tab 1 Skillpoints

local SP1 = nil
local SP2 = nil

Tab1:AddDropdown({
	Name = "Choose SP",
	Default = "",
	Options = {"10", "100","1000","10000","999999"},
	Callback = function(Value)
		SP1 = Value
	end
})

Tab1:AddButton({
	Name = "Set SP",
	Callback = function()
        StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
        wait(0.2)
        local args = {[1] = "Strength",[2] = "One",[3] = StrengthFirst,[4] = SP1}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

Tab1:AddLabel("Do not exaggerate!")

Tab1:AddTextbox({
	Name = "Custom SP",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
        StrengthFirst = game.Players.LocalPlayer.PlayerData.Strength.Value
        wait(0.1)
		SP2 = Value
	end
})

Tab1:AddButton({
	Name = "Set Custom SP",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = StrengthFirst,[4] = SP2}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end
})

