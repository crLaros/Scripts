        local args = {[1] = "Strength",[2] = "One",[3] = 0,[4] = 999999}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end    
})

Tab1:AddLabel("do not exaggerate")

Tab1:AddTextbox({
	Name = "Custom SP",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		getgenv().SP = Value
	end	  
})

Tab1:AddButton({
	Name = "You get Custom SP!",
	Callback = function()
        local args = {[1] = "Strength",[2] = "One",[3] = 0,[4] = getgenv().SP}; game:GetService("Players").LocalPlayer.RemoteFunctions.UR_ANTIHACK:FireServer(unpack(args))
  	end    
})

